{
	"scenarios": {
	    "QuickNavBookStore": {
	      "robotEnvironmentVariables": {
	        "ROS_AWS_REGION": "eu-west-1"
	      },
	      "simEnvironmentVariables": {
	        "ROS_AWS_REGION": "eu-west-1",
	        "TURTLEBOT3_MODEL": "waffle_pi",
	        "NAVIGATION_SUCCESS_COUNT": "1",
	        "SIMULATION_WORLD": "bookstore"
	      }
	    },
	    "MultiNavBookStore": {
	      "robotEnvironmentVariables": {
	        "ROS_AWS_REGION": "eu-west-1"
	      },
	      "simEnvironmentVariables": {
	        "ROS_AWS_REGION": "eu-west-1",
	        "TURTLEBOT3_MODEL": "waffle_pi",
	        "NAVIGATION_SUCCESS_COUNT": "3",
	        "SIMULATION_WORLD": "bookstore"
	      }
	    }
	},
	"simulations": [{
		"scenarios": [
			"MultiNavBookStore",
	                "QuickNavBookStore"
	             ],
		"params": {
			"failureBehavior": "Fail",
			"iamRole": "arn:aws:iam::351367772636:role/service-role/addverb_test",
			"maxJobDurationInSeconds": 600,
			"outputLocation": {
				"s3Bucket": "cogniteam-bucket",
				"s3Prefix": "test"
			},
			"robotApplications": [{
				"application": "arn:aws:robomaker:eu-west-1:351367772636:robot-application/ros-melodic-robot-app/1614504312564",
				"applicationVersion": "$LATEST",
				"launchConfig": {
					"launchFile": "await_commands.launch",
					"packageName": "cloudwatch_robot"
				}
			}],
			"simulationApplications": [{
				"application": "arn:aws:robomaker:eu-west-1:351367772636:simulation-application/ros-melodic-sim-app/1614504275746",
				"applicationVersion": "$LATEST",
				"launchConfig": {
				    "packageName": "cloudwatch_simulation",
				    "launchFile": "test_world.launch"
				}
			}]
			
		}
	}]
}
